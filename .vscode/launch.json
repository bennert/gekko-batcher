    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Import",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/import.js",
            "args": ["--config", "config.js"],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Batch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/batch.js",
            "args": ["--config", "config.js"],
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Brute Force",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/bruteforce.js",
            "args": ["--config", "config.js"],
            "console": "integratedTerminal"
        }
    ]
}